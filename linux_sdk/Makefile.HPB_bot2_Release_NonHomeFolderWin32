	-mkdir -p $(UTILS_RCBOT2_META_OBJ_DIR)




$(NAME)_$(ARCH).$(SHLIBEXT): $(UTILS_RCBOT2_META_OBJS)
	$(CLINK) $(SHLIBLDFLAGS) $(DEBUG) -o $(BUILD_DIR)/$@ $(UTILS_RCBOT2_META_OBJS) $(LDFLAGS) $(CPP_LIB)

